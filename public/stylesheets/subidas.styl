border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments
  
box-shadow()
  -webkit-box-shadow arguments
  -moz-box-shadow arguments
  box-shadow arguments

* 
  margin: 0

html
  background: gray

input.button
  //background: -webkit-gradient( linear, left top, left bottom, from(#a50aad), color-stop(0.50, #6b0d6b), to(#4a074a))
  background-color: #FFB94B
  border-radius: (3px)
  //border: 1px solid #5b139e
  border: 1px solid #FFB94B
  bottom: 20px
  box-shadow: 0px 1px 3px rgba(000,000,000,0.5), inset 0px 0px 3px rgba(255,255,255,0.4)
  //color: #ffffff
  color: #8F5A0A
  cursor: pointer
  font-size: 15px
  font-family: Arial, Helvetica
  font-style: normal
  font-variant: normal
  font-weight: blod
  padding: 8px 30px
  right: 20px
  text-shadow: 0px -1px 0px rgba(000,000,000,0.1), 0px 1px 0px rgba(145,035,145,1)

#contanier
  background-color: #808080
  height: 600px
  margin: 0 auto
  max-width: 95%

#nav
  font-weight: true
  text-align: right

h2#title
  font-size: 35px
  margin-top: 6px
  margin-bottom: 10px

p#pad
  background-color: white
  border-top-left-radius:(15px)
  border-top-right-radius:(15px)
  padding: .5%

#logos
  background-color: #CCCCCC
  border-bottom-left-radius:(15px)
  border-bottom-right-radius:(15px)
  box-shadow: -10px 10px 12px rgba(50, 50, 50, 0.85);
  padding: 10px
  text-align: center

#UploadBox
  background: #FFF
  border-radius: (11px)
  border: 1px solid #DFDFDF
  box-shadow: 0px 0px 16px 0px rgba(0,0,0,0.2)
  margin-bottom: 1%
  margin-top: 1%
  margin-left: 20%
  margin-right: 20%
  padding: 30px



